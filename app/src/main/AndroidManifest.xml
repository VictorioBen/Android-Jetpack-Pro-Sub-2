<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.submission.victorio_jetpackpro">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Victorio_jetpackpro">
        <activity
            android:parentActivityName="activity.HomeActivity"
            android:name="activity.DetailMovieActivity"
            android:label="@string/title_activity_detail_movie"
            android:theme="@style/Theme.Victorio_jetpackpro.NoActionBar"></activity>
        <activity
            android:name="activity.HomeActivity"
            android:theme="@style/Theme.MaterialComponents.NoActionBar"></activity>
        <activity
            android:name="activity.SplashScreenActivity"
            android:theme="@style/Theme.MaterialComponents.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>